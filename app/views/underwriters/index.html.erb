<p id="notice"><%= notice %></p>

<h1>Underwriters</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @underwriters.each do |underwriter| %>
      <tr>
        <td><%= underwriter.first_name %></td>
        <td><%= underwriter.last_name %></td>
        <td><%= underwriter.email %></td>
        <td><%= underwriter.company %></td>
        <td><%= link_to 'Show', underwriter %></td>
        <td><%= link_to 'Edit', edit_underwriter_path(underwriter) %></td>
        <td><%= link_to 'Destroy', underwriter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Underwriter', new_underwriter_path %>
